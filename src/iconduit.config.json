{
  "outputPath": "../artifacts/dist",
  "name": "Snout",
  "description": "Snout Router",
  "urls": {
    "base": "https://snout.dev"
  },
  "colors": {
    "background": "hsl(350, 100%, 65%)",
    "brand": "hsl(350, 100%, 85%)"
  },
  "targets": {
    "installer": []
  },
  "outputs": {
    "exclude": [
      "appleTouchStartup",
      "browserconfigXml",
      "facebookAppIcon",
      "macosIcns",
      "notificationBadge",
      "notificationIcon",
      "serviceWorker",
      "webAppIconMaskable",
      "webAppIconMasked",
      "webAppManifest",
      "windowsIco",
      "windowsTileLarge",
      "windowsTileMedium",
      "windowsTileSmall",
      "windowsTileWide"
    ]
  },
  "definitions": {
    "input": {
      "githubAccountIcon": {
        "strategy": "degrade",
        "options": {
          "to": "iconMaskableNoPadding"
        }
      }
    }
  }
}
